<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/warna_background"
    tools:context=".view.isi.AddActivity">

    <Button
        android:id="@+id/btnAddData"
        style="@style/firstbutton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="24dp"
        android:text="Tambah Data"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout7" />

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout7"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout6"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etLinkFotoTiga"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Link Foto Fasilitas"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout6"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout5"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etLinkFotoDua"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Link Foto Fasilitas"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout5"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout4"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etLinkFotoSatu"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Link Foto Fasilitas"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout4"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout3"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etLinkFoto"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Link Foto Rumah"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout3"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout2"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etAddNoHp"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Nomer HP"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout2"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="10dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textInputLayout"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etAddAlamat"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Alamat"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <com.google.android.material.textfield.TextInputLayout
        android:id="@+id/textInputLayout"
        style="@style/Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginVertical="20dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/rounded_bg"
        android:textColor="@color/warna_primary_font"
        android:textColorHint="@color/warna_primary_font"
        app:boxStrokeColor="@color/warna_background"
        app:boxStrokeWidth="0dp"
        app:hintTextColor="@color/warna_primary_font"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewAdd"
        app:shapeAppearanceOverlay="@style/Button">

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/etAddNamaKos"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:ems="10"
            android:hint="Masukkan Nama Kos"
            android:inputType="text"
            android:textColor="@color/warna_primary_font"
            android:textColorHint="@color/warna_primary_font"
            android:textSize="20sp"
            android:textStyle="normal"
            app:boxStrokeWidth="0dp" />
    </com.google.android.material.textfield.TextInputLayout>

    <TextView
        android:id="@+id/textViewAdd"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Tambah Data Kos"
        android:textColor="@color/black"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="HardcodedText" />

</androidx.constraintlayout.widget.ConstraintLayout>